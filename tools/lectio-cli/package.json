{
  "name": "lectio-cli",
  "version": "1.0.0",
  "description": "CLI tool for authenticated Lectio API access",
  "type": "module",
  "bin": {
    "lectio": "./dist/index.js"
  },
  "scripts": {
    "lectio": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "fuse.js": "^7.0.0",
    "ora": "^8.1.1",
    "puppeteer": "^24.0.0",
    "tough-cookie": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/tough-cookie": "^4.0.5",
    "typescript": "^5.7.3"
  }
}
